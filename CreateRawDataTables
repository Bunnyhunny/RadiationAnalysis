DROP TABLE IF EXISTS raw_radiation;
CREATE EXTERNAL TABLE IF NOT EXISTS raw_radiation (CapturedTime timestamp, Latitude double, Longitude double, Value BIGINT, Unit STRING, LocationName STRING, DeviceID STRING, MD5Sum STRING, Height string, Surface string, Radiation STRING, UploadedTime timestamp, LoaderID STRING)
ROW FORMAT DELIMITED FIELDS TERMINATED BY ','
STORED AS TEXTFILE LOCATION '/user/xliang18/radiation/data/masterfile/'
TBLPROPERTIES ('skip.header.line.count'='1');



DROP TABLE IF EXISTS raw_cancer_avg;
CREATE EXTERNAL TABLE IF NOT EXISTS raw_cancer_avg (County string, State string, NullNumber string, FIPS string, Objective string, IncidenceRate double, Lower double, Upper double, AnnualNumber bigint, RecentTrend string, 5YearTrendRate double, LowerInterval double, UpperInterval double)
ROW FORMAT DELIMITED FIELDS TERMINATED BY ','
STORED AS TEXTFILE LOCATION '/user/xliang18/radiation/data/cancerfile1/'
TBLPROPERTIES ('skip.header.line.count'='10');



DROP TABLE IF EXISTS raw_cancer_state;
CREATE EXTERNAL TABLE IF NOT EXISTS raw_cancer_state (AREA string, AGE_ADJUSTED_CI_LOWER double, AGE_ADJUSTED_CI_UPPER double, AGE_ADJUSTED_RATE double, COUNT bigint, EVENT_TYPE string, POPULATION bigint, RACE string, SEX string, SITE string, YEAR string, CRUDE_CI_LOWER double, CRUDE_CI_UPPER double, CRUDE_RATE double)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '|'
STORED AS TEXTFILE LOCATION '/user/xliang18/radiation/data/cancerfile2/'
TBLPROPERTIES ('skip.header.line.count'='1');
